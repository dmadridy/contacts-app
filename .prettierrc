{
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^@/types(.*)$",
    "^@/env(.*)$",
    "^@/types/(.*)$",
    "^@/constants/(.*)$",
    "^@/api/(.*)$",
    "^@/config/(.*)$",
    "^@/utils/(.*)$",
    "^@/index",
    "^@/reducers",
    "^@/slices/(.*)$",
    "^@/lib/(.*)$",
    "^@/helpers",
    "^@/hooks/(.*)$",
    "^@/pages/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^@/styles/(.*)$",
    "^@/app/(.*)$",
    "^@/assets/(.*)$",
    "^@/locales/(.*)$",
    "^[./]"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
